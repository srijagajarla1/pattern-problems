//4333334
//4322234
//4321234
//4322234
//4333334
//4444444




#include<bits/stdc++.h>
using namespace std;
int main()
{
	int i,j,n;
	cout<<"enter numbr of rows"<<endl;
	cin>>n;
	
	for(i=n;i>=1;i--)
	{
		for(j=n;j>=1;j--)
		{
			if(i>j)
			{
				cout<<i;
			}
			else
			{
				cout<<j;
			}
		}
		for(j=2;j<=n;j++)
		{
			if(i>j)
			{
				cout<<i;
			}
			else
			{
				cout<<j;
			}
		}
		cout<<endl;
	}
	for(i=2;i<=n;i++)
	{
		for(j=n;j>=1;j--)
		{
			if(i>j)
			{
				cout<<i;
			}
			else
			{
				cout<<j;
			}
		}
		for(j=2;j<=n;j++)
		{
			if(i>j)
			{
				cout<<i;
			}
			else
			{
				cout<<j;
			}
		}
		cout<<endl;
	}

}
