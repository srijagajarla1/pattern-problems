#include<bits/stdc++.h>
using namespace std;
int main()
{
	int i,j,k;
	for(i=0;i<5;i++)
	{
		for (j=4;j>=i;j--)
		{
			cout<<" ";
		}
		for (k=1;k<=2*i+1;k++)
		{
			cout<<"*";
		}
		cout<<endl;
	}
	for(i=4;i>=0;i--)
	{
		for (j=4;j>=i;j--)
		{
			cout<<" ";
		}
		for (k=2*i+1;k>0;k--)
		{
			cout<<"*";
		}
		cout<<endl;
	}
}
